{
  "name": "senter",
  "version": "0.0.1",
  "description": "A rapid data collecting application.  Think Tinder meets Pinterest",
  "main": "server/server.js",
  "scripts": {
    "start": "node dserver/server.js",
    "test": "babel-tape-runner specs/*.js",
    "lint": "jshint --exclude ./node_modules .",
    "watch": "nodemon -q -x 'npm test | tap-spec'",
    "start:dev": "nodemon server/server.js --exec babel-node",
    "build": "rm -rf dserver && babel server --out-dir dserver",
    "postinstall": "rm -rf dserver && babel server --out-dir dserver",
    "db:schema": "psql senter < server/db/scripts/schema.sql",
    "db:mock": "psql senter < server/db/scripts/mockData.sql"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/eternal44/senter"
  },
  "author": "James Youn <jamesyoun710@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-tape-runner": "^2.0.1",
    "supertest": "^1.2.0",
    "tape": "^4.5.1"
  },
  "dependencies": {
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "body-parser": "^1.15.1",
    "bootstrap": "^3.3.6",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "morgan": "^1.7.0",
    "path": "^0.12.7",
    "pg-promise": "^4.0.10",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^2.4.1",
    "redux": "^3.5.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
